<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <ThingDef ParentName="BuildingBase">
        <defName>USH_AncientJammer</defName>
        <label>ancient tower remains</label>
        <description>An ancient base of a metallic structure worn out by time. </description>
        <fillPercent>0.20</fillPercent>
        <graphicData>
            <texPath>Things/Building/AncientJammer</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(2,8)</drawSize>
            <addTopAltitudeBias>true</addTopAltitudeBias>
            <shadowData>
                <volume>(0.8, 0.7, 0.8)</volume>
                <offset>(0, 0, 0)</offset>
            </shadowData>
        </graphicData>
        <rotatable>false</rotatable>
        <drawerType>MapMeshAndRealTime</drawerType>
        <altitudeLayer>BuildingOnTop</altitudeLayer>
        <statBases>
            <MaxHitPoints>200</MaxHitPoints>
            <Mass>6</Mass>
        </statBases>
        <size>(1,1)</size>
    </ThingDef>

</Defs>